{
  "name": "Nvimlight",
  "dark": false,
  "author": "Ghostty Converter",
  "editorScheme": "/NvimLight.xml",
  "colors": {
    "primaryBackground": "#e0e2ea",
    "primaryForeground": "#14161b",
    "selectionBackground": "#9b9ea4",
    "selectionForeground": "#14161b"
  },
  "ui": {
    "*": {
      "background": "#e0e2ea",
      "foreground": "#14161b"
    },
    "Panel.background": "#d3d5dd",
    "Menu.background": "#d3d5dd",
    "MenuBar.background": "#e0e2ea",
    "ToolWindow.background": "#d3d5dd",
    "Editor.background": "#e0e2ea",
    "EditorTabs.background": "#d3d5dd",
    "Tree.background": "#e0e2ea",
    "List.background": "#e0e2ea",
    "Table.background": "#e0e2ea",
    "TextField.background": "#e0e2ea",
    "Button.background": "#d3d5dd",
    "Button.foreground": "#14161b",
    "Button.hoverBackground": "#c7c9d0",
    "Button.pressedBackground": "#afb0b6",
    "Component.borderColor": "#bbbdc3",
    "Component.focusedBorderColor": "#004c73",
    "Component.disabledForeground": "#5f6981",
    "Link.activeForeground": "#004c73",
    "Link.hoverForeground": "#004c73",
    "Link.pressedForeground": "#004c73",
    "Label.disabledForeground": "#5f6981",
    "Separator.foreground": "#bbbdc3",
    "TabbedPane.tabSelectionHeight": 2,
    "StatusBar.background": "#d3d5dd",
    "ProgressBar.progressColor": "#004c73",
    "ScrollBar.thumbColor": "#96989d",
    "SearchEverywhere.background": "#d3d5dd",
    "Notification.background": "#d3d5dd",
    "Notification.errorBackground": "#d2cbd3",
    "Notification.warningBackground": "#d4d3d2",
    "ValidationTooltip.errorBackground": "#590008",
    "ValidationTooltip.warningBackground": "#6b5300"
  }
}