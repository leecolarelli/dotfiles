  "ui": {
    "*": {
      "background": "#262b33",
      "foreground": "#c3d3de"
    },
    "Panel.background": "#39404c",
    "Menu.background": "#39404c",
    "MenuBar.background": "#262b33",
    "ToolWindow.background": "#39404c",
    "Editor.background": "#262b33",
    "EditorTabs.background": "#39404c",
    "Tree.background": "#262b33",
    "List.background": "#262b33",
    "Table.background": "#262b33",
    "TextField.background": "#262b33",
    "Button.background": "#39404c",
    "Button.foreground": "#c3d3de",
    "Button.hoverBackground": "#424b59",
    "Button.pressedBackground": "#131519",
    "Component.borderColor": "#4c5666",
    "Component.focusedBorderColor": "#576ea6",
    "Component.disabledForeground": "#697278",
    "Link.activeForeground": "#576ea6",
    "Link.hoverForeground": "#576ea6",
    "Link.pressedForeground": "#576ea6",
    "Label.disabledForeground": "#697278",
