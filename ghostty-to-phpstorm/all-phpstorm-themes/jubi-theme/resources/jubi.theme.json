{
  "name": "Jubi",
  "dark": true,
  "author": "Ghostty Converter",
  "editorScheme": "/jubi.xml",
  "colors": {
    "primaryBackground": "#262b33",
    "primaryForeground": "#c3d3de",
    "selectionBackground": "#5b5184",
    "selectionForeground": "#1e1b2e"
